{
  "name": "my-canister-dashboard",
  "version": "0.3.0-beta.1",
  "description": "Self-contained dashboard for Internet Computer canisters (BETA - not production ready)",
  "repository": "https://github.com/Web3NL/my-canister-dashboard/tree/main/canister-dashboard-frontend",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "preview": "vite preview",
    "build:deterministic": "./build-deterministic.sh",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json,css,html}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,css,html}\"",
    "typecheck": "tsc --noEmit",
    "audit": "npm audit --audit-level=high",
    "audit:fix": "npm audit fix"
  },
  "dependencies": {
    "@dfinity/agent": "2.4.1",
    "@dfinity/auth-client": "2.4.1",
    "@dfinity/cmc": "5.0.5",
    "@dfinity/ic-management": "6.1.1",
    "@dfinity/ledger-icp": "2.6.13",
    "@dfinity/principal": "2.4.1",
    "@dfinity/utils": "2.13.0"
  },
  "devDependencies": {
    "@eslint/js": "9.18.0",
    "eslint": "9.18.0",
    "typescript-eslint": "8.20.0",
    "prettier": "3.4.2",
    "typescript": "5.7.2",
    "vite": "6.3.5"
  }
}